    <SolidColorBrush x:Key="ComboBox.Disabled.Border" Color="#CCCEDB"/>
    <SolidColorBrush x:Key="ComboBox.Disabled.Button.Background" Color="#EEEEF2"/>
    <SolidColorBrush x:Key="ComboBox.Disabled.Button.Border" Color="#EEEEF2"/>
