    <SolidColorBrush x:Key="Label.Static.Text" Color="#1E1E1E" />
    <SolidColorBrush x:Key="Label.Disabled.Text" Color="#CCCEDB" />

    <!-- Focus style -->
    <SolidColorBrush x:Key="FocusStyle.Brush" Color="#1E1E1E" />
</ResourceDictionary>
